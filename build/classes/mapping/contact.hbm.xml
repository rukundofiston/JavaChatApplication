<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="beans">
  <class name="beans.Contact" table="contact">
    <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        
        <property name="idUtilisateur1" type="int">        
        </property>
        
        <property name="idUtilisateur2" type="int">        
        </property>
        
        <property name="categorie" type="java.lang.String">        
        </property>
        
        <property name="bloque" type="java.lang.String">        
        </property>
        
  
  </class>
</hibernate-mapping>
